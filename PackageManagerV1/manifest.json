{
    "manifest_version": 3,
    "name": "Plugin  Package Manager V1",
    "description": "Chrome Extension with custom div",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["content.js"]
      }
    ]
  }
  